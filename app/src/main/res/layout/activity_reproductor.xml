<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="radiochihuahua.radiochihuahua.ReproductorActivity"
    android:background="@drawable/fondo_reproductor">

    <TextView
        android:id="@+id/artista_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        app:layout_constraintBottom_toTopOf="@+id/cancion_textView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/cancion_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="455dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/album_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="471dp"
        tools:ignore="MissingConstraints" />
</android.support.constraint.ConstraintLayout>
